{
  "presets": [
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "components": "./src/components",
          "config": "./src/config",
          "constants": "./src/constants",
          "helpers": "./src/helpers",
          "services": "./src/services",
          "store": "./src/store",
          "hooks": "./src/hooks",
          "routes": "./src/routes"
        }
      }
    ]
  ]
}
